  "keywords": [
    "vapi",
    "ai",
    "voice-assistant",
    "keey",
    "airbnb",
    "property-management",
    "gohighlevel",
    "lead-qualification"
  ],
  "author": "Keey Property Management",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "libphonenumber-js": "^1.12.25",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}

